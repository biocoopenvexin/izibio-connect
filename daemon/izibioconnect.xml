<service>
	<id>izibioconnect.exe</id>
	<name>Izibio-connect</name>
	<description>Mise à jour de Alpaca à partir de Izibio.</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>--max_old_space_size=4096</argument>
	<argument>C:\Node\node-windows\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\Node\izibio-connect\app.js</argument>
	<argument>--log</argument>
	<argument>Izibio-connect wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>C:\Node\node-windows</workingdirectory>
</service>